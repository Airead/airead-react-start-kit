{
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  ecmaFeatures: {
    jsx: true,
    modules: true,
  },
  "rules": {
    "strict": 2,
    "no-unused-vars": 1,
    "semi": 2,
    "no-console": 0,
    "comma-dangle": 0,
    "react/prop-types": 0,
  },
  "env": {
      node: true,
      "browser": true
  },
  "plugins": [
    "react"
  ]
}
